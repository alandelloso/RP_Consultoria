//Muli
@import url("http://fonts.googleapis.com/css?family=Muli")

//Variables
bodybg = #EEE
color1 = #F29700
color2 = #D98700
color3 = #B26F00
color4 = #734800
color5 = #332000
facebook = #3B5999
gplus = #BC2D1D
linkedin = #2A497F
muli = 'Muli', sans-serif
textob = #DDD
textow = #555
twitter = #1AB2E8

//CSS
body
	background bodybg
	color textow
	font-family muli
	
#header
	background color1
	color textow
	padding-left 30px
	h1
		margin 0
		padding-bottom 10px
		padding-top 10px
	h2
		margin 0
		padding-bottom 10px
		
#section
	text-align center
	padding 100px 0 100px 30px
	table
		text-align center
		margin 0 auto
		td
			padding-right 100px
		img
			width 100px
			
#footer
	background color5
	text-align center
	padding 25px 0 25px 0
	h3
		color textob