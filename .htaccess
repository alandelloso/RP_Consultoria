AddDefaultCharset UTF-8

RewriteEngine on

#Principales
RewriteRule ^inicio?$ index.html
RewriteRule ^asesoramiento?$ asesoramiento.html
RewriteRule ^busquedas?$ busquedas.html
RewriteRule ^noticias?$ noticias.html
RewriteRule ^contacto?$ contacto.html

#Busquedas
RewriteRule ^busqueda/community-manager?$ sections/busqueda/comunitym.html
RewriteRule ^busqueda/ingeniero-jr-de-operaciones?$ sections/busqueda/ingjunior.html
RewriteRule ^busqueda/office-manager?$ sections/busqueda/officemanager.html
RewriteRule ^busqueda/asistente-de-facturacion?$ sections/busqueda/asistfacturacion.html
RewriteRule ^busqueda/asistente-contable?$ sections/busqueda/asistcontable.html
RewriteRule ^busqueda/ingeniero-de-desarrollo-electronico?$ sections/busqueda/ingdeselec.html

#Peru
RewriteRule ^busqueda/recepcionista-administrativa?$ sections/busqueda/recepcionista-administrativa.html

#Noticias
RewriteRule ^noticia/recursos-de-la-seguridad-social?$ sections/noticia/recursos-de-la-seguridad-social.html
RewriteRule ^noticia/resolucion-general-3653?$ sections/noticia/resolucion-afip-3653.html
RewriteRule ^noticia/pasante-en-situacion-de-embarazo?$ sections/noticia/pasante-en-situacion-de-embarazo.html
RewriteRule ^noticia/liquidacion-de-haberes?$ sections/noticia/liquidhaber.html


#Asesoramiento
RewriteRule ^informacion/recursos-humanos?$ sections/asesoramiento/recursos-humanos.html

#Alertas
RewriteRule ^success?$ sections/alerts/success.html
RewriteRule ^success-busquedas?$ sections/alerts/success-busquedas.html
RewriteRule ^fail?$ sections/alerts/fail.html

#Errores
ErrorDocument 403 /errores/403.html
ErrorDocument 404 /errores/404.html
ErrorDocument 500 /errores/500.html
ErrorDocument 503 /errores/503.html


